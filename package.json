{
	"name": "turbo-kit",
	"version": "1.1.6",
	"description": "Turborepo CLI for housekeeping",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Netail/turbo-kit.git"
	},
	"type": "module",
	"files": ["dist"],
	"engines": {
		"node": "^18.18.0 || >=20.0.0",
		"npm": ">=8.12.1"
	},
	"packageManager": "yarn@1.22.22",
	"bin": {
		"turbo-kit": "dist/cli.js"
	},
	"scripts": {
		"build": "tsup --config tsup.config.json",
		"dev": "tsup --config tsup.config.json --watch",
		"format": "biome format --write",
		"prepare": "husky || true"
	},
	"dependencies": {
		"@turbo/repository": "0.0.1-canary.10",
		"commander": "^13.1.0",
		"npm-check-updates": "~17.1.14",
		"picocolors": "^1.1.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/node": "^22.12.0",
		"husky": "^9.1.7",
		"tsup": "^8.3.6",
		"typescript": "^5.7.3"
	},
	"keywords": ["turbo", "turborepo", "monorepo"]
}
